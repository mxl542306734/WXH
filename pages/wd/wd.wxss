.information {
  width: 100%;
  height: 480rpx;
  background-color: #f35545;
  /* position: relative; */
  display: flex;
  justify-content: center;
  align-items: center;
}

.avator  image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.top_info {
  width: 80%;
}

.avator {
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx auto;
  border-radius: 50%;
}

.phone {
  width: 80%;
  margin: auto;
  text-align: center;
  color: white;
  font-size: 12pt;
  padding-bottom: 20rpx;
}

.cash {
  width: 90%;
  margin: auto;
  display: flex;
  justify-content: space-between;
}

.cash_left_top {
  text-align: center;
  font-size: 16pt;
  color: white;
  margin-bottom: 16rpx;
}

.cash_left_bot {
  text-align: center;
  font-size: 10pt;
  color: white;
}

.cash_right_top {
  font-size: 16pt;
  color: white;
  text-align: center;
  margin-bottom: 16rpx;
}

.cash_right_bot {
  font-size: 10pt;
  text-align: center;
  color: white;
}

.balance {
  border: none;
}

.balance_right {
  color: #576b95;
}

.aboutUs {
  height: 80px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 10pt;
  color: #8e8e8e;
}

.logout {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-top: 2rpx solid #d9d9d9;
  border-bottom: 2rpx solid #d9d9d9;
  margin: 12rpx 0;

}
.mask{
  position: fixed;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  background-color: rgba(0,0,0,0.5);
  z-index: 30;
   overflow: hidden; 
}

.btn_area{
  width: 100%;
  height: 400rpx;
  background-color: white;
  position: fixed;
  bottom: 0;
  overflow: hidden;
}
.btn_container{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items:  center;
}
.btn{
  width: 100%;
}
.loginBtn{
  width: 76%;
  background-color: #f35545;
  color: white;
}
.font{
  font-size: 12pt;
}
.page{
  width:100%;
  height: 100%;
  overflow: hidden;
}